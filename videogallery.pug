extends layout.pug

block content
    -var breadcrumbs = ["მთავარი","ფოტო გალერეა"]
    include breadcrumbs.pug
    section.page
        .wrapper
            -var sidebarMenu = [{name: 'ფოტო გალერეა', link: 'photogallery.html'},{name: 'ვიდეო გალერეა',link: 'videogallery.html'}]
            include sidebarmenu.pug
            .pagecontent
                .gallery
                    -for(j=0; j<9; j++)
                        .g
                            .img(style="background-image: url('images/layer-12.png')")
                                .b
                                    time 14 თებერვალი, 2018
                                    span.foto 22 ვიდეო
                            h3
                                a(href="videogalleryinner.html") ურთიერთთანამშრომლობის მემორანდუმი
